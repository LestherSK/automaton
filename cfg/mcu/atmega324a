# microcontroller configuration for atmega324a


MCU="atmega324a"

CPU_KHZ="8000"


AVRDUDE_PART=(m324a)

AVRDUDE_LFUSE="0xd7"
AVRDUDE_HFUSE="0x17"
AVRDUDE_EFUSE="0xfd"
