# avrdude configuration for atmega324a


MCU="atmega324a"

CPU_KHZ="20000"


AVRDUDE_PART=(m324a)

AVRDUDE_LFUSE="0x97"
AVRDUDE_HFUSE="0x17"
AVRDUDE_EFUSE="0xfc"
